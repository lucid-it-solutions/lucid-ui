{
  "name": "comp-lib-v5",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:js && npm run build:styles",
    "build:js": "tsup",
    "build:styles": "npm run build:scss && npm run build:tailwind && npm run combine:styles",
    "build:scss": "sass src/styles/components.scss dist/components.css --style=compressed",
    "build:tailwind": "tailwindcss -i ./src/styles/globals.css -o ./dist/tailwind.css --minify",
    "combine:styles": "cat dist/tailwind.css dist/components.css > dist/styles.css && rm dist/tailwind.css dist/components.css",
    "dev": "npm run build:styles && cd playground && npm run dev",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-styling-webpack": "^2.0.0",
    "@storybook/addon-themes": "^9.1.6",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.1",
    "@storybook/react-webpack5": "^9.1.6",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "css-loader": "^7.1.2",
    "postcss": "^8.5.6",
    "postcss-dark-theme-class": "^2.0.0",
    "postcss-scss": "^4.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sass": "^1.92.1",
    "sass-loader": "^16.0.5",
    "storybook": "^9.1.6",
    "style-loader": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.13",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^1.1.1",
    "framer": "^2.4.1",
    "lucide-react": "^0.400.0",
    "moment": "^2.30.1",
    "next-themes": "^0.4.6",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^4.1.13",
    "zod": "^4.1.9"
  }
}
